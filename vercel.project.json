{
  "name": "cricketconnect-pro",
  "version": "1.0.0",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "AUTH_SERVICE_API_URL": "@auth_service_api_url",
    "AUTH_SERVICE_API_KEY": "@auth_service_api_key"
  },
  "build": {
    "env": {
      "AUTH_SERVICE_API_URL": "@auth_service_api_url",
      "AUTH_SERVICE_API_KEY": "@auth_service_api_key"
    }
  }
}
